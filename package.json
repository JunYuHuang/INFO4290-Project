{
  "private": true,
  "name": "guessmysketch-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "lib/index.js",
  "scripts": {
    "build:server": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build:client": "cd client && npm install && npm run build",
    "start": "npm run build:client && npm run build:server && node ./lib/index.js",
    "loadtest": "colyseus-loadtest loadtest/example.ts --room my_room --numClients 2",
    "test": "node ./lib/index.js",
    "heroku-postbuild": "npm run start"
  },
  "author": "Jun Huang, Wilson Wong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.17.3",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "nodemon": "^2.0.7",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.14.0",
    "@colyseus/social": "^0.10.9",
    "bcrypt": "^5.0.1",
    "colyseus": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.1",
    "faker": "^5.4.0",
    "knex": "^0.95.2",
    "nanoid": "^3.1.20",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": "14.x"
  }
}
